{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",

    "RateLimiter": {
        "MaxRequests": 100,
        "WindowSeconds": 60
    },

    "Jwt": {
        "Issuer": "TechShopWebApi",
        "Audience": "TechShopAudience",
        "ExpiryMinutes": 60
    }
}
